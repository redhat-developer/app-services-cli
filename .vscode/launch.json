  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "cwd": "${workspaceFolder}/mas-mock",
      "request": "launch",
      "name": "Mas-Mock Debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "start",
        "debug"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Login",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "login"
      ]
    },
    {
      "name": "registry",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "registry", "artifact",   "create", "./openapitools.json"
      ]
    },
    {
      "name": "Logout",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "logout"
      ]
    },
    {
      "name": "Create Kafka",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "create",
        "my-kafka-instance"
      ]
    },
    {
      "name": "List Kafkas",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "list"
      ]
    },
    {
      "name": "Delete current Kafka",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "delete"
      ]
    },
    {
      "name": "Describe Kafka JSON",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "describe",
        "-o",
        "json"
      ]
    },
    {
      "name": "Describe Kafka YML",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "describe",
        "-o",
        "yml"
      ]
    },
    {
      "name": "List consumer groups",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "consumergroup",
        "list"
      ]
    },
    {
      "name": "Create topic",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "topic",
        "create",
        "my-topic"
      ]
    },
    {
      "name": "Delete topic",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "topic",
        "delete",
        "enda-test",
        "-y"
      ]
    },
    {
      "name": "Describe topic",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "topic",
        "describe",
        "my-topic"
      ]
    },
    {
      "name": "List topics",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "topic",
        "list"
      ]
    },
    {
      "name": "List service accounts (JSON)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "serviceaccount",
        "list",
        "-o",
        "json"
      ]
    },
    {
      "name": "List service accounts (YAML)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "serviceaccount",
        "list",
        "-o",
        "yaml"
      ]
    },
    {
      "name": "cluster connect",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "cluster",
        "connect"
      ]
    },
    {
      "name": "status",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "status"
      ]
    },
    {
      "name": "version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "version"
      ]
    }
  ]
}