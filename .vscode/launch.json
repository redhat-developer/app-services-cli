  "version": "0.2.0",
  "configurations": [
    {
      "name": "Login",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "login",
        "--url=localhost:8000"
      ]
    },
    {
      "name": "Logout",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "logout"
      ]
    },
    {
      "name": "Create Kafka",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "create",
        "--name=my-kafka-instance"
      ]
    },
    {
      "name": "List Kafkas",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "list"
      ]
    }
  ]
}