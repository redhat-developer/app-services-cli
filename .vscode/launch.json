  "version": "0.2.0",
  "configurations": [
    {
      "name": "Login",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "login"
      ]
    },
    {
      "name": "Logout",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "logout"
      ]
    },
    {
      "name": "Create Kafka",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "create",
        "--name=my-kafka-instance"
      ]
    },
    {
      "name": "List Kafkas",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "list"
      ]
    },
    {
      "name": "Describe Kafka JSON",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "describe",
        "-o",
        "json"
      ]
    },
    {
      "name": "Describe Kafka YML",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "describe",
        "-o",
        "yml"
      ]
    },
    {
      "name": "Create topic",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "kafka",
        "topics",
        "create",
        "--name=my-topic",
        "--insecure"
      ]
    },
    {
      "name": "List service accounts (JSON)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "serviceaccount",
        "list",
        "-o",
        "json"
      ]
    },
    {
      "name": "List service accounts (YAML)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "serviceaccount",
        "list",
        "-o",
        "yaml"
      ]
    },
    {
      "name": "cluster connect",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "cluster",
        "connect"
      ]
    },
    {
      "name": "status",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rhoas",
      "env": {},
      "args": [
        "status"
      ]
    }
  ]
}